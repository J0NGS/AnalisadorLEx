Class: AmericanaPizza
   
    SubClassOf: 
        NamedPizza,
        hasTopping some MozzarellaTopping,
        hasTopping some PepperoniTopping,
        hasTopping some TomatoTopping
    
    DisjointClasses: 
        AmericanaHotPizza, AmericanaPizza, MargheritaPizza, SohoPizza
    
    Individuals: 
        AmericanaPizza1,
        AmericanaPizza2
Class: NordestinaPizza
   
    SubClassOf: 
        NamedPizza,
        hasTopping some MozzarellaTopping,
        hasTopping some CarneDeSolTopping,
        hasTopping some CoalhoTopping
    
    DisjointClasses: 
        AmericanaHotPizza, AmericanaPizza, MargheritaPizza, SohoPizza
    
    Individuals: 
        NordestinnaPizza1,
        NordestinnaPizza2
Class: Pizza

    SubClassOf:
        hasBase some PizzaBase,
        hasCaloricContent some xsd:integer
    
    DisjointClasses: 
        Pizza, PizzaBase, PizzaTopping
    
    Individuals: 
        CustomPizza1,
        CustomPizza2

Class: NamedPizza

    SubClassOf: 
        Pizza


Class: NothingPizza

    SubClassOf: 

Class: Employee

    SubClassOf:
        Person
         and (hasTelefone some xsd:integer)
         and (hasMan some Man)
         and (hasTall some xsd:integer[>= 185])
Class: Person

    SubClassOf:
        Human
        or Animal
        or Living